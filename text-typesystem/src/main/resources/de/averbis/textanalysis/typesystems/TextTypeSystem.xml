			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.VerticalBar</name>
			<description>Annotation representing a vertical bar.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.DollarChar</name>
			<description>Annotation representing a dollar sign.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.EuroChar</name>
			<description>Annotation representing a euro sign.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Dash</name>
