			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Underscore</name>
			<description>Annotation representing a underscore.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Quote</name>
			<description>Annotation representing a quote.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.SingleQuote</name>
			<description>Annotation representing a single quote.</description>
			<supertypeName>de.averbis.textanalysis.types.text.Quote</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.DoubleQuote</name>
			<description>Annotation representing a double quote.</description>
			<supertypeName>de.averbis.textanalysis.types.text.Quote</supertypeName>
		</typeDescription>
		
		<!-- brackets -->
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Bracket</name>
