			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Slash</name>
			<description>Annotation representing a slash.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Backslash</name>
			<description>Annotation representing a backslash.</description>
			<supertypeName>de.averbis.extraction.types.CoreAnnotation</supertypeName>
		</typeDescription>
		<typeDescription>
			<name>de.averbis.textanalysis.types.text.Hashtag</name>
